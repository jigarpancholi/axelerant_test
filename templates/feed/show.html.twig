{% extends 'base.html.twig' %}

{% block title %}Feed{% endblock %}

{% block body %}
    <h1>Feed</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ feed.id }}</td>
            </tr>
            <tr>
                <th>Url</th>
                <td>{{ feed.url }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ feed.createdAt ? feed.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ feed.updatedAt ? feed.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('feed_index') }}">back to list</a>

    <a href="{{ path('feed_edit', {'id': feed.id}) }}">edit</a>

    {{ include('feed/_delete_form.html.twig') }}
{% endblock %}
